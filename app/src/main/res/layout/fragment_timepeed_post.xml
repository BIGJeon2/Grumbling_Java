<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <import type="android.view.View">
        </import>
        <variable
            name="timepeed_post_binding"
            type="com.bigjeon.grumbling.fragments.Fragment_TimePeed_Post" />
    </data>
<FrameLayout
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="5dp"
    tools:context="com.bigjeon.grumbling.fragments.Fragment_TimePeed_Post">
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/Timepeed_Post_RCV"
        android:layout_marginTop="6dp"
        tools:listitem="@layout/timepeed_post_item"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </androidx.recyclerview.widget.RecyclerView>
</FrameLayout>
</layout>